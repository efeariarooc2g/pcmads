/****** Object:  View [dbo].[PCMADS_COSTOBJECTASSIGNMENT]    Script Date: 11/23/2016 7:03:00 AM ******/
SET ANSI_NULLS ON
GO

SET QUOTED_IDENTIFIER ON
GO

CREATE VIEW [dbo].[PCMADS_COSTOBJECTASSIGNMENT]
AS
SELECT        dbo.PP_COSTOBJITEM.MODELID, dbo.PP_COSTOBJITEM.ID, LINEITEM.NAME AS [GL ACCOUNT], RESPCENTER.NAME AS [SENDER WBS], DRIVER.NAME AS [ALLOCATION DRIVER],
                         COSTOBJECT.NAME AS RECEIVER
FROM            (SELECT        dbo.PP_ACTIVITYDRIVER.MODELID, dbo.PP_ACTIVITYDRIVER.ID, dbo.PP_ACTIVITYDRIVER_NAME.NAME
                          FROM            dbo.PP_ACTIVITYDRIVER INNER JOIN
                                                    dbo.PP_ACTIVITYDRIVER_NAME ON dbo.PP_ACTIVITYDRIVER.MODELID = dbo.PP_ACTIVITYDRIVER_NAME.MODELID AND dbo.PP_ACTIVITYDRIVER.ID = dbo.PP_ACTIVITYDRIVER_NAME.ID
                          WHERE        (dbo.PP_ACTIVITYDRIVER.ATTRIBUTEINFO = 'N') AND (dbo.PP_ACTIVITYDRIVER.CHILDSORT <> 0)) AS DRIVER INNER JOIN
                         dbo.PP_COSTOBJECTDRIVER ON DRIVER.ID = dbo.PP_COSTOBJECTDRIVER.ACTIVITYDRIVERID AND DRIVER.MODELID = dbo.PP_COSTOBJECTDRIVER.MODELID INNER JOIN
                         dbo.PP_COSTOBJITEM ON dbo.PP_COSTOBJECTDRIVER.COSTOBJID = dbo.PP_COSTOBJITEM.ID AND dbo.PP_COSTOBJECTDRIVER.MODELID = dbo.PP_COSTOBJITEM.MODELID INNER JOIN
                             (SELECT        dbo.PP_COSTOBJECT1.MODELID, dbo.PP_COSTOBJECT1.ID, dbo.PP_COSTOBJECT1_NAME.NAME
                               FROM            dbo.PP_COSTOBJECT1 INNER JOIN
                                                         dbo.PP_COSTOBJECT1_NAME ON dbo.PP_COSTOBJECT1.MODELID = dbo.PP_COSTOBJECT1_NAME.MODELID AND dbo.PP_COSTOBJECT1.ID = dbo.PP_COSTOBJECT1_NAME.ID
                               WHERE        (dbo.PP_COSTOBJECT1.ATTRIBUTEINFO = 'N') AND (dbo.PP_COSTOBJECT1.CHILDSORT <> 0)) AS COSTOBJECT INNER JOIN
                         dbo.PP_COSTOBJECTASSIGNMENT ON COSTOBJECT.ID = dbo.PP_COSTOBJECTASSIGNMENT.COSTOBJECTID AND COSTOBJECT.MODELID = dbo.PP_COSTOBJECTASSIGNMENT.MODELID ON
                         dbo.PP_COSTOBJITEM.ID = dbo.PP_COSTOBJECTASSIGNMENT.COSTOBJID AND dbo.PP_COSTOBJITEM.MODELID = dbo.PP_COSTOBJECTASSIGNMENT.MODELID INNER JOIN
                             (SELECT        dbo.PP_LINEITEM.MODELID, dbo.PP_LINEITEM.ID, dbo.PP_LINEITEM_NAME.NAME
                               FROM            dbo.PP_LINEITEM INNER JOIN
                                                         dbo.PP_LINEITEM_NAME ON dbo.PP_LINEITEM.MODELID = dbo.PP_LINEITEM_NAME.MODELID AND dbo.PP_LINEITEM.ID = dbo.PP_LINEITEM_NAME.ID
                               WHERE        (dbo.PP_LINEITEM.ATTRIBUTEINFO = 'N') AND (dbo.PP_LINEITEM.CHILDSORT <> 0)) AS LINEITEM ON dbo.PP_COSTOBJITEM.LIACTID = LINEITEM.ID AND
                         dbo.PP_COSTOBJITEM.MODELID = LINEITEM.MODELID INNER JOIN
                             (SELECT        dbo.PP_RESPCENTER.ID, dbo.PP_RESPCENTER.MODELID, dbo.PP_RESPCENTER_NAME.NAME
                               FROM            dbo.PP_RESPCENTER INNER JOIN
                                                         dbo.PP_RESPCENTER_NAME ON dbo.PP_RESPCENTER.MODELID = dbo.PP_RESPCENTER_NAME.MODELID AND dbo.PP_RESPCENTER.ID = dbo.PP_RESPCENTER_NAME.ID
                               WHERE        (dbo.PP_RESPCENTER.ATTRIBUTEINFO = 'N') AND (dbo.PP_RESPCENTER.CHILDSORT <> 0)) AS RESPCENTER ON dbo.PP_COSTOBJITEM.RESPCENTERID = RESPCENTER.ID AND
                         dbo.PP_COSTOBJITEM.MODELID = RESPCENTER.MODELID
WHERE        (dbo.PP_COSTOBJECTDRIVER.COTYPE = 5) AND (dbo.PP_COSTOBJECTASSIGNMENT.COTYPE = 5)

GO

